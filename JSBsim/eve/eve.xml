<?xml version="1.0"?>
<!-- This is the JSBSim config file started out as the jsbsim model for the 
	eve airplane. That config file was derived from jsbsim example for a PA Seneca. 
	The reason this config file was chosen as a starting point was because it 
	had elements of RC jand a pusher aircraft, making it easy to adapt to both 
	the 6 and 30ft prototypes. AM 7/16/16 Original sections of this file were 
	generated by datcom using as much real input as available. Changes from datcom.xml 
	- function attribute NAME must be lowercase - missing function Delta_lift_due_to_flap_deflection 
	in axis LIFT - missing function Lift_due_to_Elevator_Deflection in axis LIFT -->	
	
	 
<?xml-stylesheet href="JSBSim.xsl" type="application/xml"?>
<fdm_config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	name="eve" version="1.0" release="ALPHA">

	<fileheader>
		<author>Natilus</author>
		<filecreationdate>7-16-16</filecreationdate>
		<description>Eve JSBSim Flight Dynamics Model</description>
		<reference title="Eve JSBSim Flight Dynamics Model" />
	</fileheader>

	<!-- THIS SECTION DOES NOT EXIST IN THE C172p file -->
	<output name="eve_datalog.csv" rate="60" type="CSV">
		<property> aero/qbar-psf </property>
		<property> attitude/phi-rad </property>
		<property> position/h-sl-ft </property>
		<property> velocities/vc-kts </property>
		<property> fcs/throttle-cmd-norm </property>
		<property> fcs/elevator-pos-norm </property>
		<rates> ON </rates>
		<velocities> ON </velocities>
		<forces> ON </forces>
		<moments> ON </moments>
		<position> ON </position>
		<propulsion> OFF </propulsion>
		<aerosurfaces> OFF </aerosurfaces>
		<fcs> OFF </fcs>
		<ground_reactions> ON </ground_reactions>
	</output>

	<metrics>
		<wingarea unit="FT2"> 2.2792  </wingarea>
		<wingspan unit="FT"> 4.5000  </wingspan>
		<chord unit="FT"> .5606  </chord>
		<htailarea unit="FT2"> .4457  </htailarea>
		<htailarm unit="FT"> .4280  </htailarm>
		<vtailarea unit="FT2"> .1606  </vtailarea>
		<vtailarm unit="FT"> .7127  </vtailarm>
		<!-- the aerodynamic reference point, common point for aerodynamic forces 
			(lift and drag) -->
		<location name="AERORP" unit="IN">
			<x> 4.0 </x>
			<y> 0.0 </y>
			<z> 0.0 </z>
		</location>
		<!-- the eyepoint of the pilot (needs to be of the camera) -->
		<location name="EYEPOINT" unit="IN">
			<x> 0.0 </x>
			<y> 0.0 </y>
			<z> 0.0 </z>
		</location>
		<!-- the visual reference point of the 3d model -->
		<location name="VRP" unit="IN">
			<x> 0.0 </x>
			<y> 0.0 </y>
			<z> 0.0 </z>
		</location>
	</metrics>

	<mass_balance>
		<!-- C172 model only has ixx,iyy, and izz -->
		<ixx unit="SLUG*FT2"> .1 </ixx>
		<iyy unit="SLUG*FT2"> .1 </iyy>
		<izz unit="SLUG*FT2"> .1 </izz>
		<ixy unit="SLUG*FT2"> 0 </ixy>
		<ixz unit="SLUG*FT2"> 0 </ixz>
		<iyz unit="SLUG*FT2"> 0 </iyz>
		<emptywt unit="LBS"> 2.0 </emptywt>
		<!-- the center of gravity -->
		<location name="CG" unit="IN">
			<x> 3.0 </x>
			<y> 0.0 </y>
			<z> 0.0 </z>
		</location>

		<pointmass name="ballast">
			<weight unit="LBS"> 180 </weight>
			<location name="POINTMASS" unit="IN">
				<x> 36 </x>
				<y> -14 </y>
				<z> 24 </z>
			</location>
		</pointmass>
		<pointmass name="payload">
			<weight unit="LBS"> 0 </weight>
			<location name="POINTMASS" unit="IN">
				<x> 36 </x>
				<y> 14 </y>
				<z> 24 </z>
			</location>
		</pointmass>
	</mass_balance>

